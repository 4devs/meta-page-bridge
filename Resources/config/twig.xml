<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="f_devs.meta_page.twig.meta_extension.class">FDevs\Bridge\MetaPage\Twig\MetaExtension</parameter>
    </parameters>

    <services>

        <service id="f_devs.meta_page.twig.meta_extension" class="%f_devs.meta_page.twig.meta_extension.class%">
            <argument type="service" id="f_devs.meta_page.meta_factory"/>
            <argument type="service" id="f_devs.meta_page.renderer"/>
        </service>

    </services>

</container>
